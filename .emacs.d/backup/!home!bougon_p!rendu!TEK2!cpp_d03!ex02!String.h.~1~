#ifndef STRING_H_
# define STRING_H_

typedef struct		s_string
{
  char			*str;

  // Class Methods
  void			(*assign_s)(struct s_string *, const char *);
  void			(*assign_c)(struct s_string *, const char *);
}			String;


/*
** String Class
*/
void	StringInit(String *this, const char *s);
void	StringDestroy(String *this);

#endif /* !STRING_H_ */
