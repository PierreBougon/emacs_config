#include "raytracer.h"

int	add_perf_cube(UNUSED t_data *data)
{
  if (data->itfc.add.need_form == true)
    {
      data->itfc.add.need_form = false;
      printf("ADD PERF CUBE\n");
    }
  return (0);
}
