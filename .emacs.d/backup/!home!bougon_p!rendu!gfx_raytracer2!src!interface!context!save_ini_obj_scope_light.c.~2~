#include "raytracer.h"

void	light_obj(t_data *data, t_object *obj,
		   t_buny_ini *ini, int i)
{
  char	*value;
  char	*scope;

  scope = data->itfc.save.obj_names[i];
  bunny_ini_set_field(ini, scope, "type", 0, "light");
}
