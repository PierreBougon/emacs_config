#include "sudo.c"

void	print_debug(int *tab)
{
  int   i;
  int   j;
  int   p;

  j = -1;
  p = 0;
  while (++j < 9)
    {
      i = -1;
      while (++i < 9)
        printf("%d", tab[p++]);
      printf("\n");
    }
}
