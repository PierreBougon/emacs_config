void	exec_operation(t_instruction_type intstruction_type, void *data)
{
  if (instruction_type == ADD_OPERATION)
    {
      exec_add(data->operation);

    }
  else if (instruction_type == DIV_OPERATION)
    {
      exec_div(data->operation);
    }

}
