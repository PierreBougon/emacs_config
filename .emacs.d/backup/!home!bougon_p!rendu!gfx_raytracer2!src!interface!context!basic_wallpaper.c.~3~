#include "raytracer.h"



int	basic_wallpaper(t_data *data)
{
  if (data->rt.img != NULL
      && data->click_action)
    {
      printf("CREATE WP\n");
      data->click_action = false;
    }
  else
    data->click_action = false;
  return (0);
}
