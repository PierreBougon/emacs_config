#include <stdio.h>
#include "42sh.h"

int	check_all_path(t_sh *sh, char *cmd)
{
  int	i;

  i = -1;
  while (sh->env->path[++i])
    {
      printf("%s\n", sh->env->path[i]);
    }
  return (0);
}
