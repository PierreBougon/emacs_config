#include "alum1.h"

int	main(int ac, char **av, char **env)
{
  int	size;

  if (*env == NULL)
    {
      write(2, "alum1: No environment detected\n", 31);
      exit(1);
    }
  if (ac == 1)
    size = 4;
  else
    size = my_getnbr(av[1]);
  return (0);
}
