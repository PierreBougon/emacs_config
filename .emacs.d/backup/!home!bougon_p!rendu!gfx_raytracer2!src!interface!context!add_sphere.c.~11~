#include "raytracer.h"

int	add_sphere(UNUSED t_data *data)
{
  if (data->click_action == true
      && data->mbutton == BMB_LEFT
      && data->mstate == GO_DOWN)
    {
      data->itfc.add.need_form = false;
      data->click_action = false;
      printf("ADD SPHERE WALA\n");
    }
  return (0);
}
