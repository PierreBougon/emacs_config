#include "raytracer.h"

void		save_new_ini(t_data *data)
{
  t_bunny_ini	*ini;

  ini = bunny_new_ini();
  bunny_save_ini(ini, "test.ini");
  bunny_ini_set_field(ini, "SCOPE", "field", 0 , "value");
}
