int main()
{
  int first;
  int second;
  int add_res;
  int mul_res;
  first=5;
  second=6;
  add_mul_4params(first,second,&add_res,&mul_res);
  printf("%d+%d\n",first,second,add_res);
  printf("%d*%d\n",first,second,mul_res);
  add_res=first;
  mul_res=second;
  add_mul_2param(&add_res,&mul_res);
  printf("%d+%d\n",first,second,add_res);
  printf("%d*%d\n",first,second,mul_res);
}
