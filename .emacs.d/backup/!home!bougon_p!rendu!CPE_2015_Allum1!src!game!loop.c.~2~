#include "alum1.h"

int	game_loop(int size, int **tab)
{
  char	*line;
  char	*alum;

  printf("Your turn:\nLine: ");
  line = get_next_line(0);
  printf("%s\nMatches: ", line);
  alum = get_next_line(0);
  printf("%s", alum);
  return (0);
}
