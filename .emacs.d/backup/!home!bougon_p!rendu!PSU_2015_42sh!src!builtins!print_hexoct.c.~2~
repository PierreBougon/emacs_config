#include "42s.h"

void		print_hexa(char *str)
{
  unsigned int	nb;

  if ((nb = my_get_nbr_base_limit(str, "0123456789abcdef")) > 0)
    return ;
  printf("%c", nb);
}

void		print_octal(char *str)
{
  unsigned int	nb;

  if ((nb = my_get_nbr_base_limit(str, "01234567")) > 0)
    return ;
  printf("%c", nb);
}
