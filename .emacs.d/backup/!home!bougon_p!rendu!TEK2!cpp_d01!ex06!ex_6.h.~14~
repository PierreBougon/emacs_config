#ifndef EX_6_H_
# define EX_6_H_

typedef struct	s_foo3
{
  int		foo;
  int		bar;
}		t_foo3;

typedef union	s_foo2
{
  int		foo;
  t_foo3	bar;
}		t_foo2;

typedef union	s_foo
{
  t_foo2	foo;
  int		bar;
  int		bar2;
  int		bar3;
}		t_foo;


