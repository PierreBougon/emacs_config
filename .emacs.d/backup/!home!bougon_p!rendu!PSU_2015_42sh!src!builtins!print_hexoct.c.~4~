#include "42s.h"

void		print_hexachar(char *str)
{
  unsigned int	nb;

  if ((nb = my_get_nbr_base_limit(str, "0123456789abcdef", 3)) > 0)
    return ;
  printf("%c", nb);
}

void		print_octalchar(char *str)
{
  unsigned int	nb;

  if ((nb = my_get_nbr_base_limit(str, "01234567", 2)) > 0)
    return ;
  printf("%c", nb);
}
