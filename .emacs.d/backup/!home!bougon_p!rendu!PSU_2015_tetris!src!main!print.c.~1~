void	write_tetris()
{
  printw("                                              PLEURE PAS <3\n");
  printw("*** *** *** *** * ***\n");
  printw(" *  *    *  * *   *  \n");
  printw(" *  ***  *   ** * ***\n");
  printw(" *  *    *  * * *   *\n");
  printw(" *  ***  *  * * * ***\n");
  printw("\n\n\n");
}
