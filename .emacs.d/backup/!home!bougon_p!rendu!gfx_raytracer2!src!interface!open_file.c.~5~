#include "raytracer.h"

int     open_file(t_data *data)
{
  if (data->itfc.open.need_open)
    {
      data->itfc.open.need_open = false;
    }
  else
    text(data->itfc.open.file, &data->itfc, TXT_X, TXT_Y);
  return (0);
}
