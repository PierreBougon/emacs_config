#include "shell.h"

void            show_all_tab(t_arglist *arg)
{
  t_cdlist      *tmp;
  int           j;

  tmp = arg->root->next;
  while (tmp != arg->root)
    {
      j = -1;
      while (tmp->data->cmd[++j] != NULL)
	printf("%s$\n", tmp->data->cmd[j]);
      tmp = tmp->next;
    }
}
