#include "shell.h"

int	start_shell()
{
  char	buf[4096];

  while (1)
    {
      my_bzero(buf, 4096);
      buf = get_next_line(0);
      printf("%s\n", &buf[0]);
    }
  return (0);
}

int	main(int ac, char **av, char **env)
{
  start_shell();
  return (0);
}
